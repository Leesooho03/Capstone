<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="UserSQL">
<insert id="regUser" parameterType="com.hy.capstone.user.UserVO">
	insert into user(
		user_id
		,user_pw
		,user_name
		,user_rank
	)VALUES(
		#{user_id}
		,#{user_pw}
		,#{user_name}
		,#{user_rank}
	)
</insert>

<select id="login" resultType="com.hy.capstone.user.UserVO">
	select * from user
	where user_id = #{user_id}
	and user_pw = #{user_pw}
</select>


</mapper>